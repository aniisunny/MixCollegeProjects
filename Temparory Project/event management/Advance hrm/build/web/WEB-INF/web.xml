<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>ownerreg</servlet-name>
        <servlet-class>ownerreg</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ownerlogin</servlet-name>
        <servlet-class>ownerlogin</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>clientlogin</servlet-name>
        <servlet-class>clientlogin</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Addhouse</servlet-name>
        <servlet-class>Addhouse</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>customerregis</servlet-name>
        <servlet-class>customerregis</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>customerlogin</servlet-name>
        <servlet-class>customerlogin</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>ownerreg</servlet-name>
        <url-pattern>/ownerreg</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ownerlogin</servlet-name>
        <url-pattern>/ownerlogin</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>clientlogin</servlet-name>
        <url-pattern>/clientlogin</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Addhouse</servlet-name>
        <url-pattern>/Addhouse</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>customerregis</servlet-name>
        <url-pattern>/customerregis</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>customerlogin</servlet-name>
        <url-pattern>/customerlogin</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
