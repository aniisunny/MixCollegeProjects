package CodeChef;

import java.util.ArrayList;
import java.util.List;
import java.util.Scanner;

public class XOREngine {

	public static void main(String[] args) {
		
		Scanner sc = new Scanner(System.in);
		int testCases = sc.nextInt();
		while(testCases-- != 0) {
			int n = sc.nextInt();
			int q = sc.nextInt();
			
			List<Integer> list = new ArrayList<Integer>();
			for(int i = 0; i < n; i++) list.add(sc.nextInt());
			
			List<Integer> p = new ArrayList<Integer>();
			for(int i = 0; i < q; i++) p.add(sc.nextInt());
			
			for(int i = 0; i < q; i++) {
				int temp = p.get(i);
				int evenCount = 0, oddCount = 0;
				for(int j = 0; j < n; j++) {
					int xor = temp ^ list.get(j);
					int count = Integer.bitCount(xor); 
					//String s = Integer.toBinaryString(xor);
					System.out.println(xor + " " + count);
//					int count = 0;
//					for(int k = 0; k < s.length(); k++) {
//						if(s.charAt(k) == '1') ++count;
//					}
					if(count % 2 == 0) ++evenCount;
					else ++oddCount;
				}
				System.out.println(evenCount + " " + oddCount);
			}
		}
		sc.close();

	}

}
