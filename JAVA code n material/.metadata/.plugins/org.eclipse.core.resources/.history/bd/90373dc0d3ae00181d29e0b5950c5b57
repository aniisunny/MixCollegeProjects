package Problems;

import java.util.Scanner;

public class atleastthree {
	void three(int n) {
		int og=n,counter=0;
		while(n!=0) {
			if(n%10==3) 
				counter++;
			n/=10;
		}
		if(counter>=0 && counter<=2) {
			n++;
			three(n);
		}
		else
			System.out.print("\nNearest atleast 3 three times:-" +og);
	}

	public static void main(String[] args) {
		
		Scanner sc=new Scanner(System.in);
		System.out.print("Enter the Number:-");
		int n=sc.nextInt();
		atleastthree ob=new atleastthree();
		ob.three(n);
	}
}