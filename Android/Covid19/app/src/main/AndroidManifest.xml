<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.covid19">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Quarantine"></activity>
        <activity android:name=".PrecautionsActivity" />
        <activity android:name=".SymptomsActivity" />
        <activity android:name=".FakeViralNewsActivity" />
        <activity android:name=".FAQActivity" />
        <activity android:name=".HelplineNumbersActivity" />
        <activity android:name=".MythBustersActivity" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>